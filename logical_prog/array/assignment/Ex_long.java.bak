import java.util.Arrays;
class Ex_long
{
	public static void main(String[] args) 
	{
		long[] a={109675487276l,301829865562l,501728963653l,882998652726l,472698763736l,110898072329l};
		Arrays.sort(a);
		print_a_f(a);
		print_a_b(a);
		System.out.println("***********************");
	}
	static void print_a_f(long[] x)
	{
		System.out.println("***********************");
		for (long i=0;i<x.length ;i++ )
		{
			System.out.println(x[i]);
		}
	}
	static void print_a_b(long[] x)
	{
		System.out.println("***********************");
		for (long i=x.length-1;i>=0 ;i-- )
		{
			System.out.println(x[i]);
		}
	}
}
