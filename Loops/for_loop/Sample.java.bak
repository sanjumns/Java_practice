class Sample 
{
	static void table (int a)
	{
	for (int i=1;i<=10 ;i++ )
		{
		System.out.println(a+" X "+i+" = "+(a*i));

	}
	}
	public static void main(String[] args) 
	{
		table(2);
		System.out.println("-------------------------");
		table(3);
		System.out.println("-------------------------");
		table(4);
		System.out.println("-------------------------");
		table(5);
		System.out.println("-------------------------");
	}
}
